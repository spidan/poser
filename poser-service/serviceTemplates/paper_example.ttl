@prefix ctd: <http://connectd.api/> .
@prefix onto: <http://ontodm.com/OntoDT#> .
@prefix iots: <http://iotschema.org/> .
@prefix json: <http://some.json.ontology/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <https://www.w3.org/TR/2020/CR-owl-time-20200326/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.org/> .

# Which inputs to expect and to start mapping from
json:ReificationHeader {
	ex:TimeDataSource a
		iots:TimeData; 
		rdf:predicate time:dateTime .
}

#Semantic description of the json objects to be found in the expected API

json:ApiDescription {

	ctd:JsonModel a json:Text;
		json:hasRoot ctd:Data .

	ctd:TimeStamp a json:String ;
		json:key "timestamp"^^xsd:string ;
		rdfs:isDefinedBy ex:TimeDataSource .

	ctd:Node a json:Object;
		json:key "node"^^xsd:string ;
		json:value ctd:TimeStamp .

	ctd:Edges a json:Array	;
		json:key "edges"^^xsd:string ;
		rdfs:isDefinedBy iots:TimeSeries;
		json:value ctd:Node .

	ctd:History a json:Object ;
		json:key "history"^^xsd:string;
		json:value ctd:Edges .

	ctd:Properties a json:Array ;
		json:key "properties" ;
		json:value ctd:History .

	ctd:Components a json:Array ;
		json:key "components" ;
		json:value ctd:Properties .

	ctd:Things a json:Array ;
		json:key "things";
		json:value ctd:Components .

	ctd:Data a json:Object ;
		json:key "data" ;
		json:value ctd:Things .
}